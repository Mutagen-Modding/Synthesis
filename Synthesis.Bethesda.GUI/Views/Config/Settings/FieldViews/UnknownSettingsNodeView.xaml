<local:UnknownSettingsNodeViewBase
    x:Class="Synthesis.Bethesda.GUI.Views.UnknownSettingsNodeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:local="clr-namespace:Synthesis.Bethesda.GUI.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Border Style="{StaticResource ReflectionSettingBorder}">
        <StackPanel Orientation="Horizontal">
            <TextBlock
                x:Name="SettingNameBlock"
                VerticalAlignment="Center"
                Style="{StaticResource ReflectionSettingName}" />
            <icon:PackIconModern
                Height="10"
                Margin="3,0,0,0"
                VerticalAlignment="Center"
                Foreground="{StaticResource MahApps.Brushes.Gray}"
                Kind="Question"
                ToolTip="Unknown setting type" />
        </StackPanel>
    </Border>
</local:UnknownSettingsNodeViewBase>
