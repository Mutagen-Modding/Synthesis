<UserControl
    x:Class="Mutagen.Bethesda.Synthesis.WPF.AutogeneratedSettingsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Mutagen.Bethesda.Synthesis.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <local:SettingDepthView Grid.Row="0"
            Margin="0,0,0,3"
            DataContext="{Binding SelectedSettings}" />
        <local:SettingsNodeView Grid.Row="1" DataContext="{Binding SelectedSettings}" />
    </Grid>
</UserControl>
